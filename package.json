{
  "name": "emotion-jest-lint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start",
    "unit": "jest",
    "prettier": "prettier --write .",
    "coverage": "jest --collectCoverage",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --ignore-pattern '/node_modules/' --ignore-pattern '/.next/' --ignore-pattern '/out/'",
    "verify": "yarn lint && yarn coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "aspire-components-react": "^1.0.16",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.19.0",
    "eslint-config-get-off-my-lawn": "^6.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ]
  }
}
